<h1>Event</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Eventdate</th>
            <td>{{ entity.eventDate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Numberofparticipant</th>
            <td>{{ entity.numberOfParticipant }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('event') }}">
            Back to the list
        </a>
    </li>
</ul>
